a = int(input())
b = int(input())

number_list = []
for i in range(a,b+1) :
    number_list.append(i)

perfect_squear = []
for j in number_list :
    for k in range(j) :
        if k*k == j :
            perfect_squear.append(j)
if len(perfect_squear) > 0 :
    print(perfect_squear[0])
else :
    print("No Perfect Square")
