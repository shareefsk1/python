string = input()
sub_string = input()
length = len(string)

sub_string_index = 0
for char_string in string :
    #checking it is substring or not
    if sub_string[sub_string_index] == char_string :
        sub_string_index += 1
        if sub_string_index == len(sub_string) :
            break
if sub_string_index == len(sub_string) :
    print("Yes")
else:
    print("No")
