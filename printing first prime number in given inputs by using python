No_of_inputs = int(input())


for i in range(1,No_of_inputs+1) :
    Inputs = int(input())
    is_prime = True
    if not (Inputs>1) :
        is_prime = False
        continue
    for j in range(2,Inputs) :
        if Inputs % j == 0 :
            is_prime = False
    if is_prime :
        print(Inputs)
        break
