a = int(input())

left_right_dots = a - 1
carrying_0 = 1

for i in range(1,a+1) :
    print(left_right_dots*". " + carrying_0*"0 " + left_right_dots*". ")
    left_right_dots = left_right_dots -1
    carrying_0 = carrying_0 + 2

carrying_0_in_j = (2*a-1) - 2
for j in range(1,a) :
    print(j*". " + carrying_0_in_j*"0 " + j*". ")
    carrying_0_in_j = carrying_0_in_j -2
