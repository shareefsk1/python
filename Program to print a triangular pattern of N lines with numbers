a = int(input())


for i in range(1,a+1) :
    carring_variable = ""
    for j in range(1,i+1) :
        carring_variable = carring_variable + str(j)
    carring_variable1 = ""
    counter = i-1
    while counter > 0 :
        carring_variable1 = carring_variable1 + str(counter)
        counter = counter - 1
        
    print(carring_variable + carring_variable1)
